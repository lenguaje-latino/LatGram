funcion inline()
    cinline_datos = "\nID: "..cinline.de.id.."\nNombre: "..cinline.de.nombre
    si cinline.de.apellido
        cinline_datos = cinline_datos.."\nApellido: "..cinline.de.apellido
    fin
    si cinline.de.usuario
        cinline_datos = cinline_datos.."\nUsuario: @"..cinline.de.usuario
    fin
    array = [
      textoInline(1, "Tu informaci칩n de usuario", cinline_datos),
      textoInline(2, "Tu ID", "`"..cinline.de.id.."`", falso, md)
      ]
      imprimir(array)
    enviarConsultaInline(cinline.id, array, 1)
fin

funcion callback()
   si cmatches[1] == "latino"
      link_teclado = [[
      {"text":"Github","url":"https://github.com/lenguaje-latino/BotGram"}
      ]]
      editarMensaje(cb.msg.chat.id, cb.msg.id, "*Latino* es un lenguaje de programaci칩n en espa침ol interpretado hecho en C"
      .."\n쯇uedo ayudar? Si as칤 es, *Latino busca desarrolladores* para Latino."
      .."\nCabe destacar que este bot *fu칠 programado en Latino*.", md, link_teclado)
   fin
fin

funcion mensajes()
   si matches[1] == "id"
      si !matches[2]
         imprimir(enviarMensaje(msg.chat.id, "*Chat ID*: "..msg.chat.id.."\n*User ID*: "..msg.de.id, md))
         retornar 0
      fin
      id_obtenida = obtener_id_usuario(matches[2])
      si id_obtenida
         enviarMensaje(msg.chat.id, "`"..id_obtenida.."`", md)
      sino
         enviarMensaje(msg.chat.id, "*El usuario no est치 registrado en la base de datos*.", md)
      fin
   fin
   si matches[1] == "start"
      ayuda = [[
      {"text":"Programaci칩n Latino","callback_data":"!!!latino"},
      {"text":"Repositorio del Bot","url":"https://github.com/lenguaje-latino/BotGram"}
      ]]
      enviarTeclado(msg.chat.id, "춰Hey! "..no_markdown(msg.de.nombre).."\n\nEst치s probando el nuevo bot escrito completamente en la *programaci칩n Latino*. Por el momento faltan cosas. Pero de poco a poco ir치 mejorando 游땏游녨", ayuda, md)
   fin
fin

retornar {
   "comandos": [
      "^/(id)$",
      "^/(id) @([[:alnum:]_]+)$",
      "^/(id)@"..bot.usuario.."$",
      "^/(start)$",
      "^/(start)@"..bot.usuario.."$"
   ],

   "icomandos": [
