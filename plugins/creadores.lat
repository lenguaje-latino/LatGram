funcion mensajes()
        si !es_creador(msg.de.id) retornar falso fin
        si matches[1] == "recargar"
                _BOT("recargar")
                enviarMensaje(msg.chat.id, "Bot *recargado*.", md)
        fin
        si matches[1] == "salir"
                enviarMensaje(msg.chat.id, "Bot *detenido*.", md)
                salir()
        fin
fin

retornar {d
        "comandos":[
                "/(recargar)",
                "/(salir)"
        ],
        "accion": mensajes
}
